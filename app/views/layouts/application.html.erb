<!DOCTYPE html>
<html>
  <head>
    <title>Form</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %> 
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag 'bootstrap.min', 'application', media: 'all' %>
    <%= javascript_importmap_tags %>
  </head>

  <body style="background-image: url('/assets/pexels-photo-7130498.webp');background-repeat: no-repeat; background-size: cover; ">
  <%= render partial: "shared/navbar" %> 

   <% flash.each do |name, msg|%>
   <div class="alert alert-primary" role="alert">

  <%= msg %>
  </div>
  <% end %>
    <%= yield %>
    <%= javascript_include_tag 'bootstrap.bundle.min', 'application' %>

  </body>
</html>
